    <AssemblyName>Nyami.AspNetCore.VueCliServices</AssemblyName>
    <RootNamespace>Nyami.AspNetCore.VueCliServices</RootNamespace>
    <PackageId>Nyami.AspNetCore.VueCliServices</PackageId>
    <Version>3.0.0</Version>
    <Authors>Douglas Cameron</Authors>
    <Company>Nyami</Company>
